SET(minn = 0)
SET(maxn = 1000)
SET(minx = -1000000000)
SET(maxx =  1000000000)
INT(minn, maxn, w) SPACE INT(minn, maxn, p) NEWLINE
INT(minx, maxx) SPACE INT(minx, maxx) NEWLINE
REP(w)
  INT(minx, maxx) SPACE INT(minx, maxx) SPACE INT(minx, maxx) SPACE INT(minx, maxx) NEWLINE
END
REP(p)
  INT(minx, maxx) SPACE INT(minx, maxx) SPACE INT(minx, maxx) SPACE INT(minx, maxx) NEWLINE
END
EOF
